@using Sandbox;
@using Sandbox.UI;
@namespace Reprint
@inherits FactoryPanel

<root>
	<FactoryStepPanel Fact=@(this)>
		<Name>Plus Shape</Name>
		<Config>
			<FactoryStrokeSelection @ShowConfigurator=@(workspace.useConfigurator)
				@ShowBurnSponge=@(workspace.useBurnSponge) @StrokeStep=@Fact></FactoryStrokeSelection>
		</Config>
	</FactoryStepPanel>
</root>

@code
{
	private FactoryPlus Fact { get => factory as FactoryPlus; }
	protected override FactoryStep CreateFactory() => new FactoryPlus();
}
